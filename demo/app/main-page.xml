<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded" class="page" xmlns:ui="nativescript-salesforce">
    <Page.actionBar>
        <ActionBar title="SalesForce Demo"/>
    </Page.actionBar>
    <StackLayout class="p-20">
        <Button text="Track PageView" tap="trackPageView" />
        <Button text="Fire Event" tap="fireEvent" />
        <Button text="Set Consent" tap="setConsent" />
        <Button text="Get Consent" tap="getConsent" />
        <Button text="Remove Consent" tap="removeConsent" />
    </StackLayout>
</Page>